{
   "$schema":"http://json-schema.org/schema#",
   "type":"object",
   "properties":{
      "server_key":{
         "type":"string"
      },
      "payment_type":{
         "type":"string"
      },
      "transaction_details":{
         "type":"object",
         "properties":{
            "amount":{
               "type":"integer"
            },
            "order_id":{
               "type":"integer"
            },
            "items":{
               "type":"array",
               "items":{
                  "type":"object",
                  "properties":{
                     "id":{
                        "type":"integer"
                     },
                     "name":{
                        "type":"string"
                     },
                     "price":{
                        "type":"integer"
                     },
                     "type":{
                        "type":"string"
                     },
                     "url":{
                        "type":[
                           "string",
                           "null"
                        ],
                        "format":"uri"
                     },
                     "quantity":{
                        "type":"integer"
                     }
                  },
                  "required":[
                     "id",
                     "name",
                     "price",
                     "type",
                     "quantity"
                  ]
               }
            }
         },
         "required":[
            "amount",
            "order_id",
            "items"
         ],
         "additionalProperties":false
      },
      "customer_details":{
         "type":"object",
         "properties":{
            "first_name":{
               "type":[
                  "string",
                  "null"
               ]
            },
            "last_name":{
               "type":[
                  "string",
                  "null"
               ]
            },
            "email":{
               "type":"string",
               "format":"email"
            },
            "phone":{
               "type":"string"
            }
         }
      },
      "billing_address":{
         "type":"object",
         "properties":{
            "first_name":{
               "type":"string"
            },
            "last_name":{
               "type":"string"
            },
            "address":{
               "type":"string"
            },
            "city":{
               "type":"string"
            },
            "postal_code":{
               "type":"string"
            },
            "phone":{
               "type":"string"
            },
            "country_code":{
               "type":"string"
            }
         }
      },
      "shipping_address":{
         "type":"object",
         "properties":{
            "first_name":{
               "type":"string"
            },
            "last_name":{
               "type":"string"
            },
            "address":{
               "type":"string"
            },
            "city":{
               "type":"string"
            },
            "postal_code":{
               "type":"string"
            },
            "phone":{
               "type":"string"
            },
            "country_code":{
               "type":"string"
            }
         }
      },
      "push_uri":{
         "type":"string",
         "format":"uri"
      },
      "order_status_uri":{
         "type":"string",
         "format":"uri"
      },
      "back_to_store_uri":{
         "type":"string",
         "format":"uri"
      }
   },
   "required":[
      "server_key",
      "payment_type",
      "transaction_details",
      "customer_details",
      "push_uri",
      "back_to_store_uri",
      "shipping_address",
      "billing_address"
   ],
   "additionalProperties":false
}
